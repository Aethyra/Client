<skinset name="Tooltip" image="tooltip.png">
    <widget type="Window">
        <!-- Top Row -->
        <part type="top-left-corner" xpos="0" ypos="0" width="4" height="4" />
		<part type="top-edge" xpos="4" ypos="0" width="2" height="4" />
		<part type="top-right-corner" xpos="6" ypos="0" width="4" height="4" />

		<!-- Middle Row -->
		<part type="left-edge" xpos="0" ypos="4" width="4" height="2" />
		<part type="bg-quad" xpos="4" ypos="4" width="2" height="2" />
		<part type="right-edge" xpos="6" ypos="4" width="4" height="2" />

		<!-- Bottom Row -->
		<part type="bottom-left-corner" xpos="0" ypos="6" width="4" height="4" />
		<part type="bottom-edge" xpos="4" ypos="6" width="2" height="4" />
		<part type="bottom-right-corner" xpos="6" ypos="6" width="4" height="4" />
	</widget>
</skinset>
