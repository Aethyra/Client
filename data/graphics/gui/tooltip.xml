<skinset name="Tooltip" image="tooltip.png">
    <widget type="Window">
        <!-- Top Row -->
        <part type="top-left-corner" xpos="0" ypos="0" width="2" height="2" />
		<part type="top-edge" xpos="2" ypos="0" width="1" height="2" />
		<part type="top-right-corner" xpos="3" ypos="0" width="2" height="2" />

		<!-- Middle Row -->
		<part type="left-edge" xpos="0" ypos="2" width="2" height="1" />
		<part type="bg-quad" xpos="2" ypos="2" width="1" height="1" />
		<part type="right-edge" xpos="3" ypos="2" width="2" height="1" />

		<!-- Bottom Row -->
		<part type="bottom-left-corner" xpos="0" ypos="3" width="2" height="2" />
		<part type="bottom-edge" xpos="2" ypos="3" width="1" height="2" />
		<part type="bottom-right-corner" xpos="3" ypos="3" width="2" height="2" />
	</widget>
</skinset>
